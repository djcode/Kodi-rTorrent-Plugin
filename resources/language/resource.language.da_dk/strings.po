msgctxt "Addon Summary"
msgid "rTorrent Control Plugin"
msgstr "rTorrent Kontrol-plugin"

msgctxt "Addon Description"
msgid "A simple plugin for interacting with rTorrent over HTTP or SCGI."
msgstr "Et simpelt plugin til interaktion med rTorrent via HTTP eller SCGI."

msgctxt "Addon Disclaimer"
msgid "Requires an rTorrent instance with XMLRPC enabled."
msgstr "Kræver en rTorrent-instans med XMLRPC aktiveret."

# --- SETTINGS START
msgctxt "#30000"
msgid "URI"
msgstr "URI"

msgctxt "#30001"
msgid ""
"SCGI and HTTP protocols supported.\n"
"Examples: http://host:port, scgi://host:port, scgi:///path/to/socket"
msgstr ""
"SCGI- og HTTP-protokoller understøttes.\n"
"Eksempler: http://host:port, scgi://host:port, scgi:///sti/til/socket"

msgctxt "#30004"
msgid "Downloading"
msgstr "Henter"

msgctxt "#30005"
msgid "Complete"
msgstr "Komplet"

msgctxt "#30040"
msgid "Remote Folders (If rTorrent is on a remote computer)"
msgstr "Netværksmapper (hvis rTorrent er på en netværkscomputer)"

msgctxt "#30080"
msgid "Connection"
msgstr "Forbindelse"

msgctxt "#30081"
msgid "Remote"
msgstr "Netværk"

# --- SETTINGS END
# --- CONTEXT MENU START
msgctxt "#30100"
msgid "Start Download"
msgstr "Start download"

msgctxt "#30101"
msgid "Stop Download"
msgstr "Stop download"

msgctxt "#30102"
msgid "Erase Download"
msgstr "Slet download"

msgctxt "#30120"
msgid "Set Priority: High"
msgstr "Indstil prioritet: Høj"

msgctxt "#30121"
msgid "Set Priority: Normal"
msgstr "Indstil prioritet: Normal"

msgctxt "#30122"
msgid "Set Priority: Low"
msgstr "Indstil prioritet: Lav"

msgctxt "#30123"
msgid "Set Priority: Idle"
msgstr "Indstil prioritet: Inaktiv"

msgctxt "#30124"
msgid "Set Priority: Don't Download"
msgstr "Indstil prioritet: Download ikke"

msgctxt "#30150"
msgid "Play File"
msgstr "Afspil fil"

# --- CONTEXT MENU END
# --- WARNING DIALOG START
msgctxt "#30151"
msgid "This file has not downloaded completely."
msgstr "Denne fil er ikke downloadet fuldstændigt."

msgctxt "#30152"
msgid "Are you sure you want to play it?"
msgstr "Er du sikker på at du vil afspille?"

msgctxt "#30153"
msgid "Delete Download"
msgstr "Slet download"

msgctxt "#30154"
msgid "Are you sure you want to delete this download?"
msgstr "Er du sikker på at du vil slette denne download?"

# --- WARNING DIALOG END
# --- ERROR DIALOG START
msgctxt "#30900"
msgid "Error!"
msgstr "Fejl!"

msgctxt "#30901"
msgid "Unable to play file."
msgstr "Kan ikke afspille fil."

msgctxt "#30902"
msgid "Connection Failed"
msgstr "Forbindelse mislykkedes"

msgctxt "#30903"
msgid "Check settings..."
msgstr "Kontroller indstillinger..."

msgctxt "#30904"
msgid "Exit"
msgstr "Afslut"

msgctxt "#30905"
msgid "Something went wrong."
msgstr "Noget gik galt."

msgctxt "#30906"
msgid "An XMLRPC protocol error occurred. Submit a bug report!"
msgstr "Der opstod en XMLRPC-protokolfejl. Indsend en fejlrapport!"

msgctxt "#30907"
msgid "An XMLRPC fault occurred. Submit a bug report!"
msgstr "Der opstod en XMLRPC-fejl. Indsend en fejlrapport!"

msgctxt "#30908"
msgid "Connection to rTorrent refused. Check firewall settings."
msgstr "Forbindelse til rTorrent nægtet. Kontroller indstillinger for firewall."

msgctxt "#30909"
msgid "Connection to rTorrent timed out. Check the host is correct."
msgstr "Forbindelsen til rTorrent fik timeout. Kontroller at værten er korrekt."

msgctxt "#30910"
msgid "Unexpected error occurred."
msgstr "Der opstod en uventet fejl."
