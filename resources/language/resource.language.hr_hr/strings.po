msgctxt "Addon Summary"
msgid "rTorrent Control Plugin"
msgstr "rTorrent upravljački priključak"

msgctxt "Addon Description"
msgid "A simple plugin for interacting with rTorrent over HTTP or SCGI."
msgstr "Jednostavan priključak za interakciju s rTorrent putem HTTP ili SCGI."

msgctxt "Addon Disclaimer"
msgid "Requires an rTorrent instance with XMLRPC enabled."
msgstr "Zahtijeva rTorrent primjerak s XMLRPC omogućenim."

# --- SETTINGS START
msgctxt "#30000"
msgid "URI"
msgstr "URI"

msgctxt "#30001"
msgid ""
"SCGI and HTTP protocols supported.\n"
"Examples: http://host:port, scgi://host:port, scgi:///path/to/socket"
msgstr ""
"SCGI i HTTP protokoli su podržani.\n"
"Primjeri: http://poslužitelj:ulaz, scgi://poslužitelj:ulaz, scgi:///putanja/do/priključnice"

msgctxt "#30004"
msgid "Downloading"
msgstr "Preuzimanje"

msgctxt "#30005"
msgid "Complete"
msgstr "Završeno"

msgctxt "#30040"
msgid "Remote Folders (If rTorrent is on a remote computer)"
msgstr "Udaljene mape (Ako je rTorrent na udaljenom računalu)"

msgctxt "#30080"
msgid "Connection"
msgstr "Povezivanje"

msgctxt "#30081"
msgid "Remote"
msgstr "Udaljeno"

# --- SETTINGS END
# --- CONTEXT MENU START
msgctxt "#30100"
msgid "Start Download"
msgstr "Započni preuzimanje"

msgctxt "#30101"
msgid "Stop Download"
msgstr "Zaustavi preuzimanje"

msgctxt "#30102"
msgid "Erase Download"
msgstr "Obriši preuzimanje"

msgctxt "#30120"
msgid "Set Priority: High"
msgstr "Postavi prioritet: Visok"

msgctxt "#30121"
msgid "Set Priority: Normal"
msgstr "Postavi prioritet: Normalan"

msgctxt "#30122"
msgid "Set Priority: Low"
msgstr "Postavi prioritet: Nizak"

msgctxt "#30123"
msgid "Set Priority: Idle"
msgstr "Postavi prioritet: Mirovanje"

msgctxt "#30124"
msgid "Set Priority: Don't Download"
msgstr "Postavi prioritet: Ne preuzimaj"

msgctxt "#30150"
msgid "Play File"
msgstr "Reproduciraj datoteku"

# --- CONTEXT MENU END
# --- WARNING DIALOG START
msgctxt "#30151"
msgid "This file has not downloaded completely."
msgstr "Ova datoteka nije u potpunosti preuzeta."

msgctxt "#30152"
msgid "Are you sure you want to play it?"
msgstr "Sigurno ju želite reproducirati?"

msgctxt "#30153"
msgid "Delete Download"
msgstr "Obriši preuzimanje"

msgctxt "#30154"
msgid "Are you sure you want to delete this download?"
msgstr "Sigurno želite obrisati ovo preuzimanje?"

# --- WARNING DIALOG END
# --- ERROR DIALOG START
msgctxt "#30900"
msgid "Error!"
msgstr "Greška!"

msgctxt "#30901"
msgid "Unable to play file."
msgstr "Nemoguća reprodukcija datoteke."

msgctxt "#30902"
msgid "Connection Failed"
msgstr "Neuspješno povezivanje"

msgctxt "#30903"
msgid "Check settings..."
msgstr "Provjera postavki..."

msgctxt "#30904"
msgid "Exit"
msgstr "Zatvori"

msgctxt "#30905"
msgid "Something went wrong."
msgstr "Nešto je pošlo po krivu."

msgctxt "#30906"
msgid "An XMLRPC protocol error occurred. Submit a bug report!"
msgstr "Nastala je greška XMLRPC protokola. Prijavite izvještaj greške!"

msgctxt "#30907"
msgid "An XMLRPC fault occurred. Submit a bug report!"
msgstr "Nastala je XMLRPC greška. Prijavite izvještaj greške!"

msgctxt "#30908"
msgid "Connection to rTorrent refused. Check firewall settings."
msgstr "Povezivanje s rTorrent je odbijeno. Provjerite postavke vatrozida."

msgctxt "#30909"
msgid "Connection to rTorrent timed out. Check the host is correct."
msgstr "Povezivanje s rTorrent je isteklo. Provjerite je li računalo ispravno."

msgctxt "#30910"
msgid "Unexpected error occurred."
msgstr "Dogodila se neočekivana greška."
