# --- SETTINGS START
msgctxt "#30000"
msgid "URI"
msgstr "URI"

msgctxt "#30001"
msgid "SCGI and HTTP protocols supported.\nExamples: http://host:port, scgi://host:port, scgi:///path/to/socket"
msgstr ""
"SCGI 및 HTTP 프로토콜이 지원됩니다.\n"
"예: http://host:port, scgi://host:port, scgi:///path/to/socket"

msgctxt "#30004"
msgid "Downloading"
msgstr "내려받는 중"

msgctxt "#30005"
msgid "Complete"
msgstr "완료"

msgctxt "#30040"
msgid "Remote Folders (If rTorrent is on a remote computer)"
msgstr "원격 폴더(rTorrent가 원격 컴퓨터에 있는 경우)"

msgctxt "#30080"
msgid "Connection"
msgstr "연결"

msgctxt "#30081"
msgid "Remote"
msgstr "원격"

# --- SETTINGS END
# --- CONTEXT MENU START
msgctxt "#30100"
msgid "Start Download"
msgstr "내려받기 시작"

msgctxt "#30101"
msgid "Stop Download"
msgstr "내려받기 중지"

msgctxt "#30102"
msgid "Erase Download"
msgstr "내려받기 지우기"

msgctxt "#30120"
msgid "Set Priority: High"
msgstr "우선 순위 설정: 높음"

msgctxt "#30121"
msgid "Set Priority: Normal"
msgstr "우선 순위 설정: 일반"

msgctxt "#30122"
msgid "Set Priority: Low"
msgstr "우선 순위 설정: 낮음"

msgctxt "#30123"
msgid "Set Priority: Idle"
msgstr "우선 순위 설정: 유휴"

msgctxt "#30124"
msgid "Set Priority: Don't Download"
msgstr "우선 순위 설정: 내려받지 않음"

msgctxt "#30150"
msgid "Play File"
msgstr "파일 재생"

# --- CONTEXT MENU END
# --- WARNING DIALOG START
msgctxt "#30151"
msgid "This file has not downloaded completely."
msgstr "이 파일을 완전히 내려받지 않았습니다."

msgctxt "#30152"
msgid "Are you sure you want to play it?"
msgstr "재생할까요?"

msgctxt "#30153"
msgid "Delete Download"
msgstr "내려받기 지우기"

msgctxt "#30154"
msgid "Are you sure you want to delete this download?"
msgstr "이 내려받기를 지울까요?"

# --- WARNING DIALOG END
# --- ERROR DIALOG START
msgctxt "#30900"
msgid "Error!"
msgstr "오류!"

msgctxt "#30901"
msgid "Unable to play file."
msgstr "파일을 재생할 수 없습니다."

msgctxt "#30902"
msgid "Connection Failed"
msgstr "연결 실패"

msgctxt "#30903"
msgid "Check settings..."
msgstr "설정 확인..."

msgctxt "#30904"
msgid "Exit"
msgstr "나가기"

msgctxt "#30905"
msgid "Something went wrong."
msgstr "문제가 발생했습니다."

msgctxt "#30906"
msgid "An XMLRPC protocol error occurred. Submit a bug report!"
msgstr "XMLRPC 프로토콜 오류가 발생했습니다. 버그 보고서를 제출하세요!"

msgctxt "#30907"
msgid "An XMLRPC fault occurred. Submit a bug report!"
msgstr "XMLRPC 오류가 발생했습니다. 버그 보고서를 제출하세요!"

msgctxt "#30908"
msgid "Connection to rTorrent refused. Check firewall settings."
msgstr "rTorrent에 대한 연결이 거부되었습니다. 방화벽 설정을 확인하십시오."

msgctxt "#30909"
msgid "Connection to rTorrent timed out. Check the host is correct."
msgstr "rTorrent에 대한 연결 시간이 초과되었습니다. 호스트가 올바른지 확인하십시오."

msgctxt "#30910"
msgid "Unexpected error occurred."
msgstr "예기치 않은 오류가 발생했습니다."

# --- ERROR DIALOG END
