msgctxt "Addon Summary"
msgid "rTorrent Control Plugin"
msgstr "rTorrent 控制插件"

msgctxt "Addon Description"
msgid "A simple plugin for interacting with rTorrent over HTTP or SCGI."
msgstr "通过 HTTP 或 SCGI 与 rTorrent 交互的简单插件。"

msgctxt "Addon Disclaimer"
msgid "Requires an rTorrent instance with XMLRPC enabled."
msgstr "依赖启用 XMLRPC 的 rTorrent 实例。"

# --- SETTINGS START
msgctxt "#30000"
msgid "URI"
msgstr "URI"

msgctxt "#30001"
msgid ""
"SCGI and HTTP protocols supported.\n"
"Examples: http://host:port, scgi://host:port, scgi:///path/to/socket"
msgstr ""
"支持 SCGI 和 HTTP 协议。\n"
"示例：http://host:port、scgi://host:port、scgi:///path/to/socket"

msgctxt "#30004"
msgid "Downloading"
msgstr "下载中"

msgctxt "#30005"
msgid "Complete"
msgstr "完成"

msgctxt "#30040"
msgid "Remote Folders (If rTorrent is on a remote computer)"
msgstr "远程文件夹（如果 rTorrent 位于远程计算机上）"

msgctxt "#30080"
msgid "Connection"
msgstr "连接"

msgctxt "#30081"
msgid "Remote"
msgstr "远程"

# --- SETTINGS END
# --- CONTEXT MENU START
msgctxt "#30100"
msgid "Start Download"
msgstr "开始下载"

msgctxt "#30101"
msgid "Stop Download"
msgstr "停止下载"

msgctxt "#30102"
msgid "Erase Download"
msgstr "删除下载"

msgctxt "#30120"
msgid "Set Priority: High"
msgstr "设置优先级：高"

msgctxt "#30121"
msgid "Set Priority: Normal"
msgstr "设置优先级：正常"

msgctxt "#30122"
msgid "Set Priority: Low"
msgstr "设置优先级：低"

msgctxt "#30123"
msgid "Set Priority: Idle"
msgstr "设置优先级：空闲"

msgctxt "#30124"
msgid "Set Priority: Don't Download"
msgstr "设置优先级：不下载"

msgctxt "#30150"
msgid "Play File"
msgstr "播放文件"

# --- CONTEXT MENU END
# --- WARNING DIALOG START
msgctxt "#30151"
msgid "This file has not downloaded completely."
msgstr "此文件尚未完成下载。"

msgctxt "#30152"
msgid "Are you sure you want to play it?"
msgstr "你确定要播放吗？"

msgctxt "#30153"
msgid "Delete Download"
msgstr "删除下载"

msgctxt "#30154"
msgid "Are you sure you want to delete this download?"
msgstr "你确定要删除此下载？"

# --- WARNING DIALOG END
# --- ERROR DIALOG START
msgctxt "#30900"
msgid "Error!"
msgstr "错误！"

msgctxt "#30901"
msgid "Unable to play file."
msgstr "无法播放文件。"

msgctxt "#30902"
msgid "Connection Failed"
msgstr "连接失败"

msgctxt "#30903"
msgid "Check settings..."
msgstr "检查设置..."

msgctxt "#30904"
msgid "Exit"
msgstr "退出"

msgctxt "#30905"
msgid "Something went wrong."
msgstr "出现问题。"

msgctxt "#30906"
msgid "An XMLRPC protocol error occurred. Submit a bug report!"
msgstr "发生 XMLRPC 协议错误。提交错误报告！"

msgctxt "#30907"
msgid "An XMLRPC fault occurred. Submit a bug report!"
msgstr "发生 XMLRPC 错误。提交错误报告！"

msgctxt "#30908"
msgid "Connection to rTorrent refused. Check firewall settings."
msgstr "与 rTorrent 的连接被拒绝。检查防火墙设置。"

msgctxt "#30909"
msgid "Connection to rTorrent timed out. Check the host is correct."
msgstr "与 rTorrent 的连接超时。检查主机是否正确。"

msgctxt "#30910"
msgid "Unexpected error occurred."
msgstr "发生意外错误。"
