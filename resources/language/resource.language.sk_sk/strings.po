msgctxt "Addon Summary"
msgid "rTorrent Control Plugin"
msgstr ""

msgctxt "Addon Description"
msgid "A simple plugin for interacting with rTorrent over HTTP or SCGI."
msgstr ""

msgctxt "Addon Disclaimer"
msgid "Requires an rTorrent instance with XMLRPC enabled."
msgstr ""

# --- SETTINGS START
msgctxt "#30000"
msgid "URI"
msgstr "Adresa URI"

msgctxt "#30001"
msgid ""
"SCGI and HTTP protocols supported.\n"
"Examples: http://host:port, scgi://host:port, scgi:///path/to/socket"
msgstr ""

msgctxt "#30004"
msgid "Downloading"
msgstr "Sťahovanie"

msgctxt "#30005"
msgid "Complete"
msgstr "Dokončené"

msgctxt "#30040"
msgid "Remote Folders (If rTorrent is on a remote computer)"
msgstr "Vzdialené priečinky (ak je rTorrent na vzdialenom počítači)"

msgctxt "#30080"
msgid "Connection"
msgstr "Pripojenie"

msgctxt "#30081"
msgid "Remote"
msgstr "Vzdialené"

# --- SETTINGS END
# --- CONTEXT MENU START
msgctxt "#30100"
msgid "Start Download"
msgstr "Spustiť sťahovanie"

msgctxt "#30101"
msgid "Stop Download"
msgstr "Zastaviť sťahovanie"

msgctxt "#30102"
msgid "Erase Download"
msgstr "Odstrániť stiahnutie"

msgctxt "#30120"
msgid "Set Priority: High"
msgstr "Nastaviť prioritu: Vysoká"

msgctxt "#30121"
msgid "Set Priority: Normal"
msgstr "Nastaviť prioritu: Normálna"

msgctxt "#30122"
msgid "Set Priority: Low"
msgstr "Nastaviť prioritu: Nízka"

msgctxt "#30123"
msgid "Set Priority: Idle"
msgstr "Nastaviť prioritu: Nečinné"

msgctxt "#30124"
msgid "Set Priority: Don't Download"
msgstr "Nastaviť prioritu: Nesťahovať"

msgctxt "#30150"
msgid "Play File"
msgstr "Prehrať súbor"

# --- CONTEXT MENU END
# --- WARNING DIALOG START
msgctxt "#30151"
msgid "This file has not downloaded completely."
msgstr "Tento súbor nebol kompletne stiahnutý."

msgctxt "#30152"
msgid "Are you sure you want to play it?"
msgstr "Naozaj ho chcete prehrať?"

msgctxt "#30153"
msgid "Delete Download"
msgstr "Odstrániť stiahnutie"

msgctxt "#30154"
msgid "Are you sure you want to delete this download?"
msgstr "Naozaj chcete odstrániť toto stiahnutie?"

# --- WARNING DIALOG END
# --- ERROR DIALOG START
msgctxt "#30900"
msgid "Error!"
msgstr "Chyba!"

msgctxt "#30901"
msgid "Unable to play file."
msgstr "Súbor nebolo možné prehrať."

msgctxt "#30902"
msgid "Connection Failed"
msgstr "Chyba spojenia"

msgctxt "#30903"
msgid "Check settings..."
msgstr "Skontrolujte nastavenia..."

msgctxt "#30904"
msgid "Exit"
msgstr "Ukončiť"

msgctxt "#30905"
msgid "Something went wrong."
msgstr "Niečo sa pokazilo."

msgctxt "#30906"
msgid "An XMLRPC protocol error occurred. Submit a bug report!"
msgstr ""

msgctxt "#30907"
msgid "An XMLRPC fault occurred. Submit a bug report!"
msgstr ""

msgctxt "#30908"
msgid "Connection to rTorrent refused. Check firewall settings."
msgstr ""

msgctxt "#30909"
msgid "Connection to rTorrent timed out. Check the host is correct."
msgstr ""

msgctxt "#30910"
msgid "Unexpected error occurred."
msgstr "Nastala neočakávaná chyba."
