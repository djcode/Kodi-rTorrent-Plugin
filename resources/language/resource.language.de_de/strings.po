msgctxt "Addon Summary"
msgid "rTorrent Control Plugin"
msgstr "rTorrent-Steuerung-Plugin"

msgctxt "Addon Description"
msgid "A simple plugin for interacting with rTorrent over HTTP or SCGI."
msgstr "Ein einfaches Plugin für die Zusammenarbeit mit rTorrent über HTTP oder SCGI."

msgctxt "Addon Disclaimer"
msgid "Requires an rTorrent instance with XMLRPC enabled."
msgstr "Erfordert eine rTorrent-Instanz mit aktiviertem XMLRPC."

# --- SETTINGS START
msgctxt "#30000"
msgid "URI"
msgstr "URI"

msgctxt "#30001"
msgid ""
"SCGI and HTTP protocols supported.\n"
"Examples: http://host:port, scgi://host:port, scgi:///path/to/socket"
msgstr ""
"SCGI- und HTTP-Protokolle werden unterstützt.\n"
"Beispiele: http://host:port, scgi://host:port, scgi:///pfad/zo/socket"

msgctxt "#30004"
msgid "Downloading"
msgstr "Herunterladen"

msgctxt "#30005"
msgid "Complete"
msgstr "Abgeschlossen"

msgctxt "#30040"
msgid "Remote Folders (If rTorrent is on a remote computer)"
msgstr "Remoteordner (wenn sich rTorrent auf einem entfernten Rechner befindet)"

msgctxt "#30080"
msgid "Connection"
msgstr "Verbindung"

msgctxt "#30081"
msgid "Remote"
msgstr "Remote"

# --- SETTINGS END
# --- CONTEXT MENU START
msgctxt "#30100"
msgid "Start Download"
msgstr "Download starten"

msgctxt "#30101"
msgid "Stop Download"
msgstr "Download stoppen"

msgctxt "#30102"
msgid "Erase Download"
msgstr "Download löschen"

msgctxt "#30120"
msgid "Set Priority: High"
msgstr "Priorität setzen: Hoch"

msgctxt "#30121"
msgid "Set Priority: Normal"
msgstr "Priorität setzen: Normal"

msgctxt "#30122"
msgid "Set Priority: Low"
msgstr "Priorität setzen: Niedrig"

msgctxt "#30123"
msgid "Set Priority: Idle"
msgstr "Priorität setzen: Leerlauf"

msgctxt "#30124"
msgid "Set Priority: Don't Download"
msgstr "Priorität setzen: Nicht herunterladen"

msgctxt "#30150"
msgid "Play File"
msgstr "Datei wiedergeben"

# --- CONTEXT MENU END
# --- WARNING DIALOG START
msgctxt "#30151"
msgid "This file has not downloaded completely."
msgstr "Diese Datei wurde nicht vollständig heruntergeladen."

msgctxt "#30152"
msgid "Are you sure you want to play it?"
msgstr "Soll sie wirklich wiedergegeben werden?"

msgctxt "#30153"
msgid "Delete Download"
msgstr "Download löschen"

msgctxt "#30154"
msgid "Are you sure you want to delete this download?"
msgstr "Soll dieser Download wirklich gelöscht werden?"

# --- WARNING DIALOG END
# --- ERROR DIALOG START
msgctxt "#30900"
msgid "Error!"
msgstr "Fehler!"

msgctxt "#30901"
msgid "Unable to play file."
msgstr "Datei konnte nicht wiedergegeben werden."

msgctxt "#30902"
msgid "Connection Failed"
msgstr "Verbindung fehlgeschlagen"

msgctxt "#30903"
msgid "Check settings..."
msgstr "Einstellungen werden geprüft …"

msgctxt "#30904"
msgid "Exit"
msgstr "Beenden"

msgctxt "#30905"
msgid "Something went wrong."
msgstr "Es ist etwas schief gelaufen."

msgctxt "#30906"
msgid "An XMLRPC protocol error occurred. Submit a bug report!"
msgstr "Ein XMLRPC-Protokollfehler ist aufgetreten. Bitte einen Fehlerbericht übermitteln!"

msgctxt "#30907"
msgid "An XMLRPC fault occurred. Submit a bug report!"
msgstr "Ein XMLRPC-Fehler ist aufgetreten. Bitte einen Fehlerbericht übermitteln!"

msgctxt "#30908"
msgid "Connection to rTorrent refused. Check firewall settings."
msgstr "Verbindung zu rTorrent verweigert. Bitte Firewall-Einstellungen prüfen."

msgctxt "#30909"
msgid "Connection to rTorrent timed out. Check the host is correct."
msgstr "Zeitüberschreitung bei Verbindung zu rTorrent. Bitte überprüfen, ob der Host korrekt ist."

msgctxt "#30910"
msgid "Unexpected error occurred."
msgstr "Unerwarteter Fehler aufgetreten."
