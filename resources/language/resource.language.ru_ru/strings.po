msgctxt "Addon Summary"
msgid "rTorrent Control Plugin"
msgstr ""

msgctxt "Addon Description"
msgid "A simple plugin for interacting with rTorrent over HTTP or SCGI."
msgstr ""

msgctxt "Addon Disclaimer"
msgid "Requires an rTorrent instance with XMLRPC enabled."
msgstr ""

# --- SETTINGS START
msgctxt "#30000"
msgid "URI"
msgstr "Ссылка"

msgctxt "#30001"
msgid "SCGI and HTTP protocols supported.\nExamples: http://host:port, scgi://host:port, scgi:///path/to/socket"
msgstr ""
"Поддерживаются протоколы SCGI и HTTP.\n"
"Примеры: http://host:port, scgi://host:port, scgi:///path/to/socket"

msgctxt "#30004"
msgid "Downloading"
msgstr "Загружается"

msgctxt "#30005"
msgid "Complete"
msgstr "Завершено"

msgctxt "#30040"
msgid "Remote Folders (If rTorrent is on a remote computer)"
msgstr "Удалённые папки (Если rTorrent - удалённый компьютер)"

msgctxt "#30080"
msgid "Connection"
msgstr "Соединение"

msgctxt "#30081"
msgid "Remote"
msgstr ""

# --- SETTINGS END
# --- CONTEXT MENU START
msgctxt "#30100"
msgid "Start Download"
msgstr "Начать загрузку"

msgctxt "#30101"
msgid "Stop Download"
msgstr "Остановить загрузку"

msgctxt "#30102"
msgid "Erase Download"
msgstr "Удалить загрузку"

msgctxt "#30120"
msgid "Set Priority: High"
msgstr "Приоритет: Высокий"

msgctxt "#30121"
msgid "Set Priority: Normal"
msgstr "Приоритет: Нормальный"

msgctxt "#30122"
msgid "Set Priority: Low"
msgstr "Приоритет: Низкий"

msgctxt "#30123"
msgid "Set Priority: Idle"
msgstr "Приоритет: В простое"

msgctxt "#30124"
msgid "Set Priority: Don't Download"
msgstr "Приоритет: Не скачивать"

msgctxt "#30150"
msgid "Play File"
msgstr "Воспроизвести файл"

# --- CONTEXT MENU END
# --- WARNING DIALOG START
msgctxt "#30151"
msgid "This file has not downloaded completely."
msgstr "Этот файл не загружен полностью."

msgctxt "#30152"
msgid "Are you sure you want to play it?"
msgstr "Вы уверены, что хотите воспроизвести этот файл?"

msgctxt "#30153"
msgid "Delete Download"
msgstr "Удалить загрузку"

msgctxt "#30154"
msgid "Are you sure you want to delete this download?"
msgstr "Вы уверены, что хотите удалить эту загрузку?"

# --- WARNING DIALOG END
# --- ERROR DIALOG START
msgctxt "#30900"
msgid "Error!"
msgstr "Ошибка!"

msgctxt "#30901"
msgid "Unable to play file."
msgstr "Не удалось воспроизвести файл."

msgctxt "#30902"
msgid "Connection Failed"
msgstr "Потерянно соединение"

msgctxt "#30903"
msgid "Check settings..."
msgstr "Проверка настроек..."

msgctxt "#30904"
msgid "Exit"
msgstr "Выход"

msgctxt "#30905"
msgid "Something went wrong."
msgstr "Что-то пошло не так."

msgctxt "#30906"
msgid "An XMLRPC protocol error occurred. Submit a bug report!"
msgstr "Произошла ошибка протокола XMLRPC. Напишите отчёт об ошибке!"

msgctxt "#30907"
msgid "An XMLRPC fault occurred. Submit a bug report!"
msgstr "Произошла ошибка XMLRPC. Напишите отчёт об ошибке!"

msgctxt "#30908"
msgid "Connection to rTorrent refused. Check firewall settings."
msgstr "Соединение с rTorrent сброшено. Проверьте настройки брандмауэра."

msgctxt "#30909"
msgid "Connection to rTorrent timed out. Check the host is correct."
msgstr "Истекло время на ожидание соединения с rTorrent. Проверьте адрес хоста."

msgctxt "#30910"
msgid "Unexpected error occurred."
msgstr "Произошла неожиданная ошибка."

# --- ERROR DIALOG END
