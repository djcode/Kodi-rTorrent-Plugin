msgctxt "Addon Summary"
msgid "rTorrent Control Plugin"
msgstr "Plugin de controle rTorrent"

msgctxt "Addon Description"
msgid "A simple plugin for interacting with rTorrent over HTTP or SCGI."
msgstr "Um plugin simples para interagir com rTorrent através dos protocolos HTTP ou SCGI."

msgctxt "Addon Disclaimer"
msgid "Requires an rTorrent instance with XMLRPC enabled."
msgstr "Requer uma instância rTorrent com XMLRPC ativado."

# --- SETTINGS START
msgctxt "#30000"
msgid "URI"
msgstr "URI"

msgctxt "#30001"
msgid ""
"SCGI and HTTP protocols supported.\n"
"Examples: http://host:port, scgi://host:port, scgi:///path/to/socket"
msgstr ""
"Protocolos SCGI e HTTP suportados.\n"
"Exemplos: http://host:port, scgi://host:port, scgi:///path/to/socket"

msgctxt "#30004"
msgid "Downloading"
msgstr "Baixando"

msgctxt "#30005"
msgid "Complete"
msgstr "Concluído"

msgctxt "#30040"
msgid "Remote Folders (If rTorrent is on a remote computer)"
msgstr "Pastas remotas (caso o rTorrent esteja em um computador remoto)"

msgctxt "#30080"
msgid "Connection"
msgstr "Conexão"

msgctxt "#30081"
msgid "Remote"
msgstr "Remoto"

# --- SETTINGS END
# --- CONTEXT MENU START
msgctxt "#30100"
msgid "Start Download"
msgstr "Iniciar Download"

msgctxt "#30101"
msgid "Stop Download"
msgstr "Parar Download"

msgctxt "#30102"
msgid "Erase Download"
msgstr "Apagar Download"

msgctxt "#30120"
msgid "Set Priority: High"
msgstr "Definir Prioridade: Alta"

msgctxt "#30121"
msgid "Set Priority: Normal"
msgstr "Definir Prioridade: Normal"

msgctxt "#30122"
msgid "Set Priority: Low"
msgstr "Definir Prioridade: Baixa"

msgctxt "#30123"
msgid "Set Priority: Idle"
msgstr "Definir Prioridade: Ocioso"

msgctxt "#30124"
msgid "Set Priority: Don't Download"
msgstr "Definir Prioridade: Não baixar"

msgctxt "#30150"
msgid "Play File"
msgstr "Reproduzir Arquivo"

# --- CONTEXT MENU END
# --- WARNING DIALOG START
msgctxt "#30151"
msgid "This file has not downloaded completely."
msgstr "Este arquivo não foi baixado completamente."

msgctxt "#30152"
msgid "Are you sure you want to play it?"
msgstr "Tem certeza de que deseja reproduzir?"

msgctxt "#30153"
msgid "Delete Download"
msgstr "Excluir Download"

msgctxt "#30154"
msgid "Are you sure you want to delete this download?"
msgstr "Tem certeza de que deseja excluir este download?"

# --- WARNING DIALOG END
# --- ERROR DIALOG START
msgctxt "#30900"
msgid "Error!"
msgstr "Erro!"

msgctxt "#30901"
msgid "Unable to play file."
msgstr "Não foi possível reproduzir o arquivo."

msgctxt "#30902"
msgid "Connection Failed"
msgstr "Falha na conexão"

msgctxt "#30903"
msgid "Check settings..."
msgstr "Verifique as configurações..."

msgctxt "#30904"
msgid "Exit"
msgstr "Sair"

msgctxt "#30905"
msgid "Something went wrong."
msgstr "Algo deu errado."

msgctxt "#30906"
msgid "An XMLRPC protocol error occurred. Submit a bug report!"
msgstr "Ocorreu um erro de protocolo XMLRPC. Envie um relatório de erro!"

msgctxt "#30907"
msgid "An XMLRPC fault occurred. Submit a bug report!"
msgstr "Ocorreu uma falha XMLRPC. Envie um relatório de erro!"

msgctxt "#30908"
msgid "Connection to rTorrent refused. Check firewall settings."
msgstr "Conexão com rTorrent recusada. Verifique as configurações do firewall."

msgctxt "#30909"
msgid "Connection to rTorrent timed out. Check the host is correct."
msgstr "A conexão com rTorrent expirou. Verifique se o host está correto."

msgctxt "#30910"
msgid "Unexpected error occurred."
msgstr "Ocorreu um erro inesperado."
