msgctxt "Addon Summary"
msgid "rTorrent Control Plugin"
msgstr "Connector pel control de rTorrent"

msgctxt "Addon Description"
msgid "A simple plugin for interacting with rTorrent over HTTP or SCGI."
msgstr "Un complement senzill per interactuar amb rTorrent mitjançant HTTP o SCGI."

msgctxt "Addon Disclaimer"
msgid "Requires an rTorrent instance with XMLRPC enabled."
msgstr "Requereix una instància rTorrent amb XMLRPC habilitat."

# --- SETTINGS START
msgctxt "#30000"
msgid "URI"
msgstr "URI"

msgctxt "#30001"
msgid ""
"SCGI and HTTP protocols supported.\n"
"Examples: http://host:port, scgi://host:port, scgi:///path/to/socket"
msgstr ""
"S'admeten protocols SCGI i HTTP.\n"
"Exemples: http://host:port, scgi://host:port, scgi:///path/to/socket"

msgctxt "#30004"
msgid "Downloading"
msgstr "Baixant"

msgctxt "#30005"
msgid "Complete"
msgstr "Completat"

msgctxt "#30040"
msgid "Remote Folders (If rTorrent is on a remote computer)"
msgstr "Carpetes Remotes (si rTorrent es troba en un ordinador remot)"

msgctxt "#30080"
msgid "Connection"
msgstr "Connexió"

msgctxt "#30081"
msgid "Remote"
msgstr "Remot"

# --- SETTINGS END
# --- CONTEXT MENU START
msgctxt "#30100"
msgid "Start Download"
msgstr "Inicia la baixada"

msgctxt "#30101"
msgid "Stop Download"
msgstr "Atura la baixada"

msgctxt "#30102"
msgid "Erase Download"
msgstr "Esborra la baixada"

msgctxt "#30120"
msgid "Set Priority: High"
msgstr "Estableix prioritat: Alta"

msgctxt "#30121"
msgid "Set Priority: Normal"
msgstr "Estableix prioritat: Normal"

msgctxt "#30122"
msgid "Set Priority: Low"
msgstr "Estableix prioritat: Baixa"

msgctxt "#30123"
msgid "Set Priority: Idle"
msgstr "Estableix prioritat: Inactiu"

msgctxt "#30124"
msgid "Set Priority: Don't Download"
msgstr "Estableix prioritat: No baixar"

msgctxt "#30150"
msgid "Play File"
msgstr "Reproduir Fitxer"

# --- CONTEXT MENU END
# --- WARNING DIALOG START
msgctxt "#30151"
msgid "This file has not downloaded completely."
msgstr "Aquest fitxer no s'ha baixat del tot."

msgctxt "#30152"
msgid "Are you sure you want to play it?"
msgstr "Segur que el voleu reproduir?"

msgctxt "#30153"
msgid "Delete Download"
msgstr "Esborra la baixada"

msgctxt "#30154"
msgid "Are you sure you want to delete this download?"
msgstr "Segur que vols suprimir aquesta baixada?"

# --- WARNING DIALOG END
# --- ERROR DIALOG START
msgctxt "#30900"
msgid "Error!"
msgstr "Error!"

msgctxt "#30901"
msgid "Unable to play file."
msgstr "Impossible reproduir el fitxer."

msgctxt "#30902"
msgid "Connection Failed"
msgstr "La connexió ha fallat"

msgctxt "#30903"
msgid "Check settings..."
msgstr "Comprova la configuració..."

msgctxt "#30904"
msgid "Exit"
msgstr "Sortir"

msgctxt "#30905"
msgid "Something went wrong."
msgstr "Alguna cosa ha anat malament."

msgctxt "#30906"
msgid "An XMLRPC protocol error occurred. Submit a bug report!"
msgstr "Error del protocol XMLRPC. Envia l'informe de l'error!"

msgctxt "#30907"
msgid "An XMLRPC fault occurred. Submit a bug report!"
msgstr "S'ha produït un error del XMLRPC. Envia l'informe de l'error!"

msgctxt "#30908"
msgid "Connection to rTorrent refused. Check firewall settings."
msgstr "S'ha rebutjat la connexió a rTorrent. Comprova la configuració del tallafoc."

msgctxt "#30909"
msgid "Connection to rTorrent timed out. Check the host is correct."
msgstr "La connexió a rTorrent ha esgotat el temps d'espera. Comprova que l'amfitrió és correcte."

msgctxt "#30910"
msgid "Unexpected error occurred."
msgstr "Hi ha hagut un error inesperat."
