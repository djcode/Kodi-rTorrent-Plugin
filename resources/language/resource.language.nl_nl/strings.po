msgctxt "Addon Summary"
msgid "rTorrent Control Plugin"
msgstr "rTorrent Control Plugin"

msgctxt "Addon Description"
msgid "A simple plugin for interacting with rTorrent over HTTP or SCGI."
msgstr "Een eenvoudige plug-in voor interactie met rTorrent via HTTP of SCGI."

msgctxt "Addon Disclaimer"
msgid "Requires an rTorrent instance with XMLRPC enabled."
msgstr "Vereist een rTorrent-instantie met XMLRPC ingeschakeld."

# --- SETTINGS START
msgctxt "#30000"
msgid "URI"
msgstr "URI"

msgctxt "#30001"
msgid ""
"SCGI and HTTP protocols supported.\n"
"Examples: http://host:port, scgi://host:port, scgi:///path/to/socket"
msgstr ""
"SCGI- en HTTP-protocollen worden ondersteund.\n"
"Voorbeelden: http://host:poort, scgi://host:poort, scgi:///pad/naar/socket"

msgctxt "#30004"
msgid "Downloading"
msgstr "Downloaden"

msgctxt "#30005"
msgid "Complete"
msgstr "Compleet"

msgctxt "#30040"
msgid "Remote Folders (If rTorrent is on a remote computer)"
msgstr "Externe mappen (als rTorrent op een externe computer staat)"

msgctxt "#30080"
msgid "Connection"
msgstr "Verbinding"

msgctxt "#30081"
msgid "Remote"
msgstr "Op afstand"

# --- SETTINGS END
# --- CONTEXT MENU START
msgctxt "#30100"
msgid "Start Download"
msgstr "Start Download"

msgctxt "#30101"
msgid "Stop Download"
msgstr "Stop Download"

msgctxt "#30102"
msgid "Erase Download"
msgstr "Wis Download"

msgctxt "#30120"
msgid "Set Priority: High"
msgstr "Prioriteit instellen: Hoog"

msgctxt "#30121"
msgid "Set Priority: Normal"
msgstr "Prioriteit instellen: Normaal"

msgctxt "#30122"
msgid "Set Priority: Low"
msgstr "Prioriteit instellen: Laag"

msgctxt "#30123"
msgid "Set Priority: Idle"
msgstr "Prioriteit instellen: Inactief"

msgctxt "#30124"
msgid "Set Priority: Don't Download"
msgstr "Prioriteit instellen: Niet Downloaden"

msgctxt "#30150"
msgid "Play File"
msgstr "Bestand afspelen"

# --- CONTEXT MENU END
# --- WARNING DIALOG START
msgctxt "#30151"
msgid "This file has not downloaded completely."
msgstr "Dit bestand is niet volledig gedownload."

msgctxt "#30152"
msgid "Are you sure you want to play it?"
msgstr "Weet je zeker dat je het wilt afspelen?"

msgctxt "#30153"
msgid "Delete Download"
msgstr "Verwijder Download"

msgctxt "#30154"
msgid "Are you sure you want to delete this download?"
msgstr "Weet je zeker dat je deze download wilt verwijderen?"

# --- WARNING DIALOG END
# --- ERROR DIALOG START
msgctxt "#30900"
msgid "Error!"
msgstr "Fout!"

msgctxt "#30901"
msgid "Unable to play file."
msgstr "Kan bestand niet afspelen."

msgctxt "#30902"
msgid "Connection Failed"
msgstr "Verbinding mislukt"

msgctxt "#30903"
msgid "Check settings..."
msgstr "Controleer instellingen..."

msgctxt "#30904"
msgid "Exit"
msgstr "Uitgang"

msgctxt "#30905"
msgid "Something went wrong."
msgstr "Er is iets fout gegaan."

msgctxt "#30906"
msgid "An XMLRPC protocol error occurred. Submit a bug report!"
msgstr "Er is een XMLRPC-protocolfout opgetreden. Dien een bugrapport in!"

msgctxt "#30907"
msgid "An XMLRPC fault occurred. Submit a bug report!"
msgstr "Er is een XMLRPC-fout opgetreden. Dien een bugrapport in!!"

msgctxt "#30908"
msgid "Connection to rTorrent refused. Check firewall settings."
msgstr "Verbinding met rTorrent geweigerd. Controleer de firewall-instellingen."

msgctxt "#30909"
msgid "Connection to rTorrent timed out. Check the host is correct."
msgstr "Verbinding met rTorrent is verlopen. Controleer of de host correct is."

msgctxt "#30910"
msgid "Unexpected error occurred."
msgstr "Er is een onverwachte fout opgetreden."
