msgctxt "Addon Summary"
msgid "rTorrent Control Plugin"
msgstr "rTorrent-ohjauslisäosa"

msgctxt "Addon Description"
msgid "A simple plugin for interacting with rTorrent over HTTP or SCGI."
msgstr "Yksinkertainen lisäosa rTorrentin ohjaukseen HTTP-/SCGI-protokollien välityksellä."

msgctxt "Addon Disclaimer"
msgid "Requires an rTorrent instance with XMLRPC enabled."
msgstr "Vaatii rTorrent-instanssin, jossa on käytössä XMLRPC."

# --- SETTINGS START
msgctxt "#30000"
msgid "URI"
msgstr "URI"

msgctxt "#30001"
msgid ""
"SCGI and HTTP protocols supported.\n"
"Examples: http://host:port, scgi://host:port, scgi:///path/to/socket"
msgstr ""
"Tukee SCGI- ja HTTP-protokollia.\n"
"Esimerkkejä: http://host:port, scgi://host:port, scgi:///path/to/socket"

msgctxt "#30004"
msgid "Downloading"
msgstr "Ladataan"

msgctxt "#30005"
msgid "Complete"
msgstr "Valmis"

msgctxt "#30040"
msgid "Remote Folders (If rTorrent is on a remote computer)"
msgstr "Etäkansiot (jos rTorrent sijaitsee etäkoneella)"

msgctxt "#30080"
msgid "Connection"
msgstr "Yhteys"

msgctxt "#30081"
msgid "Remote"
msgstr "Etäsijainti"

# --- SETTINGS END
# --- CONTEXT MENU START
msgctxt "#30100"
msgid "Start Download"
msgstr "Käynnistä lataus"

msgctxt "#30101"
msgid "Stop Download"
msgstr "Pysäytä lataus"

msgctxt "#30102"
msgid "Erase Download"
msgstr "Poista lataus"

msgctxt "#30120"
msgid "Set Priority: High"
msgstr "Aseta painotus: Korkea"

msgctxt "#30121"
msgid "Set Priority: Normal"
msgstr "Aseta painotus: Normaali"

msgctxt "#30122"
msgid "Set Priority: Low"
msgstr "Aseta painotus: Matala"

msgctxt "#30123"
msgid "Set Priority: Idle"
msgstr "Aseta painotus: Toimeton"

msgctxt "#30124"
msgid "Set Priority: Don't Download"
msgstr "Aseta painotus: Älä lataa"

msgctxt "#30150"
msgid "Play File"
msgstr "Toista tiedosto"

# --- CONTEXT MENU END
# --- WARNING DIALOG START
msgctxt "#30151"
msgid "This file has not downloaded completely."
msgstr "Tämä tiedosto on ladattu kokonaan."

msgctxt "#30152"
msgid "Are you sure you want to play it?"
msgstr "Haluatko varmasti toistaa sen?"

msgctxt "#30153"
msgid "Delete Download"
msgstr "Poista lataus"

msgctxt "#30154"
msgid "Are you sure you want to delete this download?"
msgstr "Haluatko varmasti poistaa latauksen?"

# --- WARNING DIALOG END
# --- ERROR DIALOG START
msgctxt "#30900"
msgid "Error!"
msgstr "Virhe!"

msgctxt "#30901"
msgid "Unable to play file."
msgstr "Tiedostoa ei voitu toistaa."

msgctxt "#30902"
msgid "Connection Failed"
msgstr "Yhteyden muodostus epäonnistui"

msgctxt "#30903"
msgid "Check settings..."
msgstr "Tarkista asetukset..."

msgctxt "#30904"
msgid "Exit"
msgstr "Sulje"

msgctxt "#30905"
msgid "Something went wrong."
msgstr "Jotain meni pieleen."

msgctxt "#30906"
msgid "An XMLRPC protocol error occurred. Submit a bug report!"
msgstr "Tapahtui XMLRPC-protokollavirhe. Lähetä virheilmoitus!"

msgctxt "#30907"
msgid "An XMLRPC fault occurred. Submit a bug report!"
msgstr "Tapahtui XMLRPC-virhe. Lähetä virheilmoitus!"

msgctxt "#30908"
msgid "Connection to rTorrent refused. Check firewall settings."
msgstr "rTorrent-yhteys evättiin. Tarkista palomuurin asetukset."

msgctxt "#30909"
msgid "Connection to rTorrent timed out. Check the host is correct."
msgstr "rTorrent-yhteys aikakatkaistiin. Tarkista palvelimen tiedot."

msgctxt "#30910"
msgid "Unexpected error occurred."
msgstr "Tapahtui odottamaton virhe."
