msgctxt "Addon Summary"
msgid "rTorrent Control Plugin"
msgstr "Plug-in de contrôle rTorrent"

msgctxt "Addon Description"
msgid "A simple plugin for interacting with rTorrent over HTTP or SCGI."
msgstr "Un plugin simple pour interagir avec rTorrent via HTTP ou SCGI."

msgctxt "Addon Disclaimer"
msgid "Requires an rTorrent instance with XMLRPC enabled."
msgstr "Nécessite une instance rTorrent avec XMLRPC activé."

# --- SETTINGS START
msgctxt "#30000"
msgid "URI"
msgstr "URI"

msgctxt "#30001"
msgid ""
"SCGI and HTTP protocols supported.\n"
"Examples: http://host:port, scgi://host:port, scgi:///path/to/socket"
msgstr ""
"Protocoles SCGI et HTTP pris en charge.\n"
"Exemples : http://host:port, scgi://host:port, scgi:///path/to/socket"

msgctxt "#30004"
msgid "Downloading"
msgstr "Téléchargement"

msgctxt "#30005"
msgid "Complete"
msgstr "Complet"

msgctxt "#30040"
msgid "Remote Folders (If rTorrent is on a remote computer)"
msgstr "Dossiers distants (si rTorrent est sur un ordinateur distant)"

msgctxt "#30080"
msgid "Connection"
msgstr "Connexion"

msgctxt "#30081"
msgid "Remote"
msgstr "Hôte"

# --- SETTINGS END
# --- CONTEXT MENU START
msgctxt "#30100"
msgid "Start Download"
msgstr "Commencer Le téléchargement"

msgctxt "#30101"
msgid "Stop Download"
msgstr "Arrêter le téléchargement"

msgctxt "#30102"
msgid "Erase Download"
msgstr "Effacer le téléchargement"

msgctxt "#30120"
msgid "Set Priority: High"
msgstr "Définir la priorité : Élevée"

msgctxt "#30121"
msgid "Set Priority: Normal"
msgstr "Définir la priorité : Normale"

msgctxt "#30122"
msgid "Set Priority: Low"
msgstr "Définir la priorité : Lente"

msgctxt "#30123"
msgid "Set Priority: Idle"
msgstr "Définir la priorité : Inactive"

msgctxt "#30124"
msgid "Set Priority: Don't Download"
msgstr "Définir la priorité : Ne pas télécharger"

msgctxt "#30150"
msgid "Play File"
msgstr "Lire le fichier"

# --- CONTEXT MENU END
# --- WARNING DIALOG START
msgctxt "#30151"
msgid "This file has not downloaded completely."
msgstr "Ce fichier n'a pas été entièrement téléchargé."

msgctxt "#30152"
msgid "Are you sure you want to play it?"
msgstr "Êtes-vous sûr de vouloir le lire ?"

msgctxt "#30153"
msgid "Delete Download"
msgstr "Supprimer le téléchargement"

msgctxt "#30154"
msgid "Are you sure you want to delete this download?"
msgstr "Voulez-vous vraiment supprimer ce téléchargement ?"

# --- WARNING DIALOG END
# --- ERROR DIALOG START
msgctxt "#30900"
msgid "Error!"
msgstr "Erreur !"

msgctxt "#30901"
msgid "Unable to play file."
msgstr "Impossible de lire le fichier."

msgctxt "#30902"
msgid "Connection Failed"
msgstr "La connexion a échoué"

msgctxt "#30903"
msgid "Check settings..."
msgstr "Vérifier les paramètres..."

msgctxt "#30904"
msgid "Exit"
msgstr "Sortir"

msgctxt "#30905"
msgid "Something went wrong."
msgstr "Quelque chose s'est mal passé."

msgctxt "#30906"
msgid "An XMLRPC protocol error occurred. Submit a bug report!"
msgstr "Une erreur de protocole XMLRPC s'est produite. Soumettre un rapport de bogue !"

msgctxt "#30907"
msgid "An XMLRPC fault occurred. Submit a bug report!"
msgstr "Une erreur XMLRPC s'est produite. Soumettre un rapport de bogue !"

msgctxt "#30908"
msgid "Connection to rTorrent refused. Check firewall settings."
msgstr "Connexion à rTorrent refusée. Vérifier les paramètres du pare-feu."

msgctxt "#30909"
msgid "Connection to rTorrent timed out. Check the host is correct."
msgstr "La connexion à rTorrent a expiré. Vérifier que l'hôte est correct."

msgctxt "#30910"
msgid "Unexpected error occurred."
msgstr "Une erreur inattendue s'est produite."
