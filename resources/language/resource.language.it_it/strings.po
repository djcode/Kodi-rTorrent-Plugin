msgctxt "Addon Summary"
msgid "rTorrent Control Plugin"
msgstr "Plugin di controllo rTorrent"

msgctxt "Addon Description"
msgid "A simple plugin for interacting with rTorrent over HTTP or SCGI."
msgstr "Un semplice plugin per interagire con rTorrent su HTTP o SCGI."

msgctxt "Addon Disclaimer"
msgid "Requires an rTorrent instance with XMLRPC enabled."
msgstr "Richiede un'istanza rTorrent con XMLRPC abilitato."

# --- SETTINGS START
msgctxt "#30000"
msgid "URI"
msgstr "URI"

msgctxt "#30001"
msgid ""
"SCGI and HTTP protocols supported.\n"
"Examples: http://host:port, scgi://host:port, scgi:///path/to/socket"
msgstr ""
"Protocolli SCGI e HTTP supportati. \n"
"Esempi: http://host:porta, scgi://host:porta, scgi:///percorso/socket"

msgctxt "#30004"
msgid "Downloading"
msgstr "Download"

msgctxt "#30005"
msgid "Complete"
msgstr "Completo"

msgctxt "#30040"
msgid "Remote Folders (If rTorrent is on a remote computer)"
msgstr "Cartelle remote (se rTorrent è su un computer remoto)"

msgctxt "#30080"
msgid "Connection"
msgstr "Connessione"

msgctxt "#30081"
msgid "Remote"
msgstr "Remoto"

# --- SETTINGS END
# --- CONTEXT MENU START
msgctxt "#30100"
msgid "Start Download"
msgstr "Inizia download"

msgctxt "#30101"
msgid "Stop Download"
msgstr "Ferma download"

msgctxt "#30102"
msgid "Erase Download"
msgstr "Cancella download"

msgctxt "#30120"
msgid "Set Priority: High"
msgstr "Imposta priorità: alta"

msgctxt "#30121"
msgid "Set Priority: Normal"
msgstr "Imposta priorità: normale"

msgctxt "#30122"
msgid "Set Priority: Low"
msgstr "Imposta priorità: bassa"

msgctxt "#30123"
msgid "Set Priority: Idle"
msgstr "Imposta priorità: inattivo"

msgctxt "#30124"
msgid "Set Priority: Don't Download"
msgstr "Imposta priorità: non scaricare"

msgctxt "#30150"
msgid "Play File"
msgstr "Riproduci file"

# --- CONTEXT MENU END
# --- WARNING DIALOG START
msgctxt "#30151"
msgid "This file has not downloaded completely."
msgstr "Questo file non è stato scaricato completamente."

msgctxt "#30152"
msgid "Are you sure you want to play it?"
msgstr "Sei sicuro di volerlo riprodurre?"

msgctxt "#30153"
msgid "Delete Download"
msgstr "Elimina download"

msgctxt "#30154"
msgid "Are you sure you want to delete this download?"
msgstr "Sei sicuro di voler eliminare questo download?"

# --- WARNING DIALOG END
# --- ERROR DIALOG START
msgctxt "#30900"
msgid "Error!"
msgstr "Errore!"

msgctxt "#30901"
msgid "Unable to play file."
msgstr "Impossibile riprodurre il file."

msgctxt "#30902"
msgid "Connection Failed"
msgstr "Connessione fallita"

msgctxt "#30903"
msgid "Check settings..."
msgstr "Controlla le impostazioni..."

msgctxt "#30904"
msgid "Exit"
msgstr "Esci"

msgctxt "#30905"
msgid "Something went wrong."
msgstr "Qualcosa è andato storto."

msgctxt "#30906"
msgid "An XMLRPC protocol error occurred. Submit a bug report!"
msgstr "Si è verificato un errore del protocollo XMLRPC. Invia una segnalazione di bug!"

msgctxt "#30907"
msgid "An XMLRPC fault occurred. Submit a bug report!"
msgstr "Si è verificato un errore XMLRPC. Invia una segnalazione di bug!"

msgctxt "#30908"
msgid "Connection to rTorrent refused. Check firewall settings."
msgstr "Connessione a rTorrent rifiutata. Verifica le impostazioni del firewall."

msgctxt "#30909"
msgid "Connection to rTorrent timed out. Check the host is correct."
msgstr "Connessione a rTorrent scaduta. Verifica che l'host sia corretto."

msgctxt "#30910"
msgid "Unexpected error occurred."
msgstr "Si è verificato un errore imprevisto."
